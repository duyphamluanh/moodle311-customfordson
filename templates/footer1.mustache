                <div class="col-xs-12 col-sm-4 col-md-4 contact">
                    <div class="panel-group" id="mainfooterdiv1" role="tablist" aria-multiselectable="true">
                        <div class="panelfooter">
                            <h3>{{#str}} office:footer, theme_fordson {{/str}}</h3>
                            <div class="panel-tools pull-right">
                                <button id="footermaindiv1" type="button" class="btn btn-box-tool d-md-none d-lg-none" data-toggle="collapse" data-target="#collapsemainfooterdiv1" aria-expanded="true">
                                </button>
                            </div>
                        </div>
                        <div id="collapsemainfooterdiv1" class="panel-collapse collapse show" role="tabpanel">
                            <div class="panel-body">  
                            <p><i class="fa fa-home" aria-hidden="true"></i>{{#str}} address:footer, theme_fordson {{/str}}</p>
                            </div>
                        </div>
                    </div>      
                    <div class="brandbox">
                        {{#output.brandorganization_footer}}<h3>{{output.brandorganization_footer}}</h3>{{/output.brandorganization_footer}}
                            <ul class="footercontact">
                                
                            </ul>

                        {{{ output.social_icons }}}

                    </div>
                </div>
                
                <div class="col-xs-12 col-sm-4 col-md-4 contact">
